{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": [
    "plugin:fsd/all",
    "airbnb-typescript/base",
    "plugin:jest/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "tsconfigRootDir": "."
  },
  "plugins": ["@typescript-eslint", "fsd", "jest"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off"
  }
}